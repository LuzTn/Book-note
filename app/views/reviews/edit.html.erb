<h1> Edit Review </h1>
<%= simple_form_for([@book, @review]) do |f| %>
  <div id="rating-form">
    <label> Rating</label>
  </div>
  <%= f.input :comment %>
  <%= f.button :submit %>
<% end %>
        <script>
          $('#rating-form').raty({
            path: '/app/assets/images/',
            scoreName: 'review[rating]'
          });
        </script>